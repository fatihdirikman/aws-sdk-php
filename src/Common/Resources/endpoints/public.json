[
  {
    "priority": 500,
    "regionPrefix": "cn-",
    "rules": [
      {
        "services": ["iam", "sts"],
        "config": {
          "signatureVersion": "v4",
          "endpoint": "{scheme}://{service}.cn-north-1.amazonaws.com.cn"
        }
      },
      {
        "config": {
          "signatureVersion": "v4",
          "endpoint": "{scheme}://{service}.{region}.amazonaws.com.cn"
        }
      }
    ]
  },
  {
    "priority": 600,
    "regionPrefix": "us-gov-",
    "rules": [
      {
        "services": ["iam"],
        "config": {
          "endpoint": "{scheme}://{service}.us-gov.amazonaws.com"
        }
      },
      {
        "config": {
          "endpoint": "{scheme}://{service}.{region}.us-gov.amazonaws.com"
        }
      }
    ]
  },
  {
    "priority": 900,
    "regionPrefix": "us-east-1",
    "rules": [
      {
        "services": ["s3", "sdb"],
        "config": {
          "endpoint": "{scheme}://{service}.amazonaws.com"
        }
      }
    ]
  },
  {
    "priority": 999,
    "regionPrefix": "",
    "rules": [
      {
        "services": ["s3"],
        "config": {
          "endpoint": "{scheme}://{service}-{region}.amazonaws.com"
        }
      },
      {
        "services": ["cloudfront", "iam", "importexport", "route53", "sts"],
        "config": {
          "endpoint": "{scheme}://{service}.amazonaws.com"
        }
      },
      {
        "config": {
          "endpoint": "{scheme}://{service}.{region}.amazonaws.com"
        }
      }
    ]
  }
]
