{
    "name": "aws/aws-sdk-php",
    "homepage": "http://aws.amazon.com/sdkforphp",
    "description": "AWS SDK for PHP - Use Amazon Web Services in your PHP project",
    "keywords": ["aws","amazon","sdk","s3","ec2","dynamodb","cloud","glacier"],
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Amazon Web Services",
            "homepage": "http://aws.amazon.com"
        }
    ],
    "support": {
        "forum": "https://forums.aws.amazon.com/forum.jspa?forumID=80",
        "issues": "https://github.com/aws/aws-sdk-php/issues"
    },
    "require": {
        "php": ">=5.4",
        "guzzlehttp/guzzle": "~5.0@dev",
        "guzzlehttp/command": "dev-async",
        "guzzlehttp/message-integrity-subscriber": "dev-master",
        "guzzlehttp/retry-subscriber": "dev-guzzle5",
        "guzzlehttp/log-subscriber": "dev-master",
        "mtdowling/jmespath.php": "~1.1"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "ext-openssl": "*",
        "phpunit/phpunit": "4.*"
    },
    "suggest": {
        "ext-openssl": "Allows working with CloudFront private distributions and verifying received SNS messages"
    },
    "autoload": {
        "psr-4": {
            "Aws\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
